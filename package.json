{
  "name": "labs-server-node",
  "version": "2.3.4",
  "description": "Labs: Server (node)",
  "main": "index.ts",
  "scripts": {
    "prestart": "npm install",
    "start": "node dist/index.js",
    "build": "npm run build:live",
    "clean": "rm -f ./dist",
    "build:copy": "cpx \"src/**/*.{yaml,json,html}\" dist/",
    "build:live": "npm run clean && tsc -p tsconfig.json && npm run build:copy",
    "build:dev": "nodemon --ignore ./test/** --ignore ./cache/ --ignore ./docs/ --ignore ./log/ --exec ts-node -- ./src/index.ts",
    "dev": "npm run build:dev",
    "lint": "tslint --project ./ > log/lint.errors.txt",
    "test": "./node_modules/.bin/mocha --require ts-node/register test/*",
    "test:win": ".\\node_modules\\.bin\\mocha --require ts-node/register .\\test\\**\\*.specs.js  --timeout 160000",
    "docs": "@powershell -Command scripts/generate_docs.ps1"
  },
  "private": true,
  "dependencies": {
    "@analytics/google-analytics": "^0.5.3",
    "@analytics/google-tag-manager": "^0.5.1",
    "@jmarroyave/fw-console": "^1.0.9",
    "analytics": "^0.7.16",
    "axios": "^0.21.4",
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.3",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.12.0",
    "express-request-id": "^1.4.1",
    "express-requests-logger": "^3.0.3",
    "express-winston": "^4.2.0",
    "g": "^2.0.1",
    "graphql": "^15.5.3",
    "js-yaml": "^3.3.0",
    "mongoose": "^6.0.5",
    "morgan": "^1.10.0",
    "node-uuid": "^1.4.8",
    "oas-tools": "^2.1.4",
    "openapi-to-graphql": "^2.5.1",
    "perfy": "^1.1.5",
    "response-time": "^2.3.2",
    "serve-index": "^1.9.1",
    "socket.io": "^4.1.2",
    "tslint": "^6.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-graphql": "^0.9.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.9.1",
    "@types/node-uuid": "^0.0.29",
    "@types/winston": "^2.4.4",
    "chai": "^4.3.4",
    "chai-openapi-response-validator": "^0.13.0",
    "ts-lint": "^4.5.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
