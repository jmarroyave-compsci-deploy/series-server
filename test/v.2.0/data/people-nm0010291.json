  "body": {
    "operationName": "getPerson",
    "variables": {
      "id": "nm0010291"
    },
    "query": "fragment PersonInfoTiny on Person {\n  id\n  name\n  __typename\n}\n\nfragment MovieInfoTiny on Movie {\n  id\n  title\n  releaseYear\n  directed {\n    id {\n      ...PersonInfoTiny\n      __typename\n    }\n    __typename\n  }\n  __typename\n}\n\nquery getPerson($id: String!) {\n  person(id: $id) {\n    id\n    name\n    birthYear\n    deathYear\n    profession\n    awards {\n      category\n      festival {\n        id\n        name\n        __typename\n      }\n      won\n      year\n      __typename\n    }\n    references {\n      imdb\n      __typename\n    }\n    produced {\n      id {\n        ...MovieInfoTiny\n        __typename\n      }\n      __typename\n    }\n    directed {\n      id {\n        ...MovieInfoTiny\n        __typename\n      }\n      __typename\n    }\n    acted {\n      id {\n        ...MovieInfoTiny\n        __typename\n      }\n      as\n      __typename\n    }\n    wrote {\n      id {\n        ...MovieInfoTiny\n        __typename\n      }\n      __typename\n    }\n    crew {\n      id {\n        ...MovieInfoTiny\n        __typename\n      }\n      as\n      cat\n      job\n      __typename\n    }\n    __typename\n  }\n}\n"
  }
}